<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gradetracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GradeTracker">
        <activity android:name=".addGradeActivity" />
        <activity android:name=".addAssignmentActivity" />
        <activity android:name=".sixthSubjectActivity" />
        <activity android:name=".fifthSubjectActivity" />
        <activity android:name=".fourthSubjectActivity" />
        <activity android:name=".thirdSubjectActivity" />
        <activity android:name=".secondSubjectActivity" />
        <activity android:name=".firstGradeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>